# Gokul
#include<stdio.h>
#include <stdlib.h>

int main()
{
char s[100];
int n,i,j;
scanf("%s%n",s,&n);
for(i=0;i<n;i++)
{
    for(j=0;j<n;j++)
    {
        if(i==j)
        {
            printf("%c",s[j]);
        }
        else if(i==n-j-1)
        {
            printf("%c",s[n-i-1]);
            
        }
        else
        {
            printf(" ");
        }
    }
    printf("\n");
}

}
