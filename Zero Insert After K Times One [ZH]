#include<stdio.h>
#include <stdlib.h>
int main()
{
int n,m,i,c=0;
scanf("%d %d",&n,&m);
int a[n];
for(i=0;i<n;i++)
{
    scanf("%d",&a[i]);
    if(a[i]==1)
      {
        c++;
        }
    else
    {
        c=0;
    }
    
   
    printf("%d ",a[i]);
    if(c==m)
    {
        printf("0 ");
        c=0;
    }

}

}
