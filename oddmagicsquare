#include <iostream>
 
using namespace std;

int main(int argc, char** argv)
{
    int n,i,j,row,col,a[30][30] ;
    cin>>n;
    if (n%2==0)
    {
        cout<<"invalid";
        
    }
    else
    {
    col = (n+1)/2 ;
    row = 1;
    for(i=1;i<=n*n;i++)
    {
        a[row][col]=i ;
        if(i%n==0)
        {
            row+=1 ;
            goto loop;       
        }
        if(row==1)
        {
            row=n ;
        }
        else
        {
            row-=1 ;
        
        }
        if(col == n)
        {
            col=1;
        }
        else
        {   
            col+=1 ;
            loop:;        
        }
        
    }
    for (i=1;i<=n;i++)
        {
        for (j=1;j<=n;j++)
        {
            cout<<a[i][j]<<' ';
        }
        cout<<"\n";
    }
}
}
