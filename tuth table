#include<stdio.h>

int main() 
{
  int i,j,k,arr[10][10][10];
  printf("A B C O/P\n");
  for(i=0; i<=1; i++)
  {
      for(j=0; j<=1; j++)
      {
          for(k=0; k<=1 ;k++)
          {
                arr[i][j][k]=i||j||k;
                printf("%d %d %d  %d\n",i,j,k,arr[i][j][k]);
              
          }
      }
  }
}
