#include<stdio.h>

int main()
{
   int number,rot,num,pos=0,j;
   int a[100],i=0,temp;
   scanf("%d %d",&number,&rot);
   num=number;
   while(num > 0)
   {
       a[i]=num%10;
       num=num/10;
       i++;
   }
   pos= i;
   for(i=0;i<rot;i++)
   {
   temp=a[pos-1];
   for(j=pos-2;j>=0;j--)
   {
        a[j+1]=a[j];
   }
   a[0]=temp;
   }
   for(i=pos-1;i>=0;i--)
   {
    printf("%d",a[i]);
   }
}
