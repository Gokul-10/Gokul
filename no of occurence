#include<stdio.h>
#include <stdlib.h>

int main()
{
    int a[100],i=0;
    while(scanf("%d",&a[i])==1)
    {
        
        i++;
    }
int n,j,count[i],k;
n=i;
for(j=0;j<n;j++)
{
    count[j]=1;
    for(k=j+1;k<n;k++)
    {
        if(a[k]==a[j]&&a[j]>=0)
        {
            count[j]++;
            a[k]=-2;

        
        }
    }
if(a[j]!=-2)
  {
      printf("%d --> %d\n",a[j],count[j]);
  }
}    

}
